@model Polyteks.Katman.TefrikBildirim.Models.IkModel

@{
    ViewBag.Title = "PersonelSicilIslemler";
    Layout = "~/Views/Shared/Layout.cshtml";
}


@if (TempData["Msg"] != null)
{
    <div class="custom-alerts alert alert-@TempData["class"] fade in">
        <button type="button" class="close" data-dismiss="alert" aria-hidden="true"></button>
        <h2>@Html.Raw(TempData["Msg"])</h2>
    </div>
}

<div class="row">
    <div class="col-md-12 ">
        <div class="portlet light bordered">
            <div class="portlet-title">
                <div class="caption font-blue">
                    <i class="icon-settings font-blue"></i>


                    <span class="caption-subject bold uppercase">PERSONEL EĞİTİM SİCİL MODÜLÜ</span>
                </div>
            </div>
            <div class="portlet-body form">
                @Html.Partial("~/Views/InsanKaynaklari/_IkBaseButonlar.cshtml", Model)
                <br/>
                <div class="row">
                    <h2 class=" text-center font-red-thunderbird"><b>LÜTFEN İLGİLİ ALANI SEÇİNİZ</b></h2>
                </div>
            </div>
        </div>
    </div>
</div>
